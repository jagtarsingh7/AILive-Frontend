  "license": "MIT",
  "scripts": {
    "main-app": "nx serve main-app --host=0.0.0.0",
    "start": "nx serve",
    "build": "nx build",
    "lint": "nx run-many --target=lint --verbose --parallel=3",
    "storybook": "nx run components:storybook",
    "gen:theme-typings": "chakra-cli tokens libs/components/src/theme/index.ts --strict-component-types",
