  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "nx affected --target=lint --all",
      "nx format:write --base"
    ]
  },
